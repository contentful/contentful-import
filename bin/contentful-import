#!/usr/bin/env node
var runContentfulImport = require('../dist/index')
var usageParams = require('../dist/usageParams')

runContentfulImport(usageParams)
.then((result) => {
  process.exit(0)
})
.catch(function (err) {
  if (err.name !== 'ContentfulMultiError') {
    console.error(err)
  }
  process.exit(1)
})
